
header {
    'RuqqJS'
    a.github-ribbon href='https://github.com/tenbits/RuqqJS' >
        img src='https://s3.amazonaws.com/github/ribbons/forkme_right_darkblue_121621.png';
}

scroller.content#maskScroller {
    p.container.tabPanel {
        div.info.active {
            div > 'Here are helper classes for "daily dev". '
            div.hint > 'This Lib is still in work, as it is pretty dirty.'
            
            ul {
                li {
                    code > 'ruqq.arr'
                    div > 'Array Helpers'
                }
                li {
                    code > 'ruqq.animate'
                    div > 'Animation Helpers / CssSprite Animation'
                }
                li {
                    code > 'ruqq.events'
                    div > 'Event Helpers'
                }                
                li {
                    code > 'utils'
                    div > 'Object. , String. , Date. Helpers'
                }
                li {
                    code > 'ruqq.net.URI'
                    div > 'URI Class'
                }
            }
            
        }      
        div.api > 'Placeholder for RuqqJS Documentation'
    }
}

footer {
    radioButtons{
        button.active name='info' > 'Info'
        button name='api' > 'API'        
    }
}